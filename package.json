{
  "name": "poi-islands-of-ireland-api",
  "version": "1.0.0",
  "description": "## A point of interest service api using Node and Hapi framework",
  "main": "index.js",
  "scripts": {
    "star": "node index.js",
    "test": "npm run test:regions && npm run test:users",
    "test:regions": "mocha --ui tdd --exit --timeout 5000 test/regions-api-test.js",
    "test:users": "mocha --ui tdd --exit --timeout 5000 test/users-api-test.js",
    "cover": "npm run cover:regions && npm run cover:users && npm run cover:report",
    "cover:regions": "nyc --silent mocha --ui tdd --exit --timeout 5000 test/regions-api-test.js",
    "cover:users": "nyc --silent --no-clean mocha --ui tdd --exit --timeout 5000 test/users-api-test.js",
    "cover:report": "nyc report --reporter=lcov --reporter=text"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peter-dev/poi-islands-of-ireland-api.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/peter-dev/poi-islands-of-ireland-api/issues"
  },
  "homepage": "https://github.com/peter-dev/poi-islands-of-ireland-api#readme",
  "dependencies": {
    "@hapi/hapi": "^18.3.1",
    "bcrypt": "^3.0.6",
    "boom": "^7.3.0",
    "dotenv": "^7.0.0",
    "hapi": "^18.1.0",
    "hapi-auth-jwt2": "^8.4.0",
    "hapi-swagger": "^9.4.2",
    "inert": "^5.1.2",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "mais-mongoose-seeder": "^1.0.7",
    "mongoose": "^5.4.20",
    "vision": "^5.4.4"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "prettier": "^1.16.4"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120
  }
}
